      "type": "choice",
      "answerOption": [
        { "valueString": "A.\t Security" },
        { "valueString": "B.\t Interoperability" },
        { "valueString": "C.\t Sustainability" },
        { "valueString": "D.\t Responsibility" }
      ]
    },
